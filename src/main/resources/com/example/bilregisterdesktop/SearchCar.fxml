<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bilregisterdesktop.Controller.SearchCarController">
    <Label text="Søk etter bil ved registreringsnummer" />
    <TextField fx:id="regNumSearchField" promptText="Registreringsnummer" />
    <Button onAction="#searchByRegNum" text="Søk" />
    <TableView fx:id="searchResultTable" prefHeight="432.0" prefWidth="786.0">
        <columns>
            <TableColumn fx:id="idColumn" text="ID" />
            <TableColumn fx:id="modelColumn" text="Model" />
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="statusColumn" text="Status" />
            <TableColumn fx:id="regNumColumn" text="RegNum" />
            <TableColumn fx:id="customer_idColumn" text="Customer ID" />
            <TableColumn fx:id="priceColumn" text="Price" />
            <TableColumn fx:id="kmColumn" text="Km" />
            <TableColumn fx:id="year_modelColumn" text="Year Model" /> <!-- Legg til denne linjen -->
        </columns>
    </TableView>
</VBox>

