<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="506.0" prefWidth="538.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bilregisterdesktop.Controller.UpdateController">
    <Label text="Select Car" />
    <ComboBox fx:id="carComboBox" promptText="Choose a car" />

    <Label text="Update Status" />
    <TextField fx:id="statusTextField" />
    <Button onAction="#updateCarStatus" text="Update" />

    <Label text="Update Price" />
    <TextField fx:id="priceTextField" />
    <Button onAction="#updateCarPrice" text="Update" />

    <Label text="Update KM" />
    <TextField fx:id="kmTextField" />
    <Button onAction="#updateCarKM" text="Update" />
</VBox>
